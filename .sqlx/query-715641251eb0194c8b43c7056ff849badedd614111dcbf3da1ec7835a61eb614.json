  "query": "\n        select \n            b.bookmark_id,\n            b.url,\n            b.title,\n            b.description,\n            b.created_at,\n            u.username as created_by\n        from bookmarks b\n        join users u on b.user_id = u.user_id\n        where b.user_id = ? and b.is_archived = 0\n        order by b.created_at desc\n        limit ? offset ?\n        ",
